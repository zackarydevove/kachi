{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:setup": "ts-node scripts/setup-test-db.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:migrate": "DATABASE_URL=TEST_DATABASE_URL prisma migrate deploy",
    "build": "npm install && npx prisma migrate deploy && tsc && npx prisma generate",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint 'src/**/*.ts'",
    "create-snapshots": "ts-node -r tsconfig-paths/register scripts/create-snapshots.ts",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma migrate reset --force"
  },
