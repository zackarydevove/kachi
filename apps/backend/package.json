    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.9",
    "@types/pg": "^8.16.0",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
